{
  "name": "@workshop/backend",
  "version": "1.0.0",
  "description": "Workshop Backend API with Authentication, User Management, Email System and Oracle integration",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "type-check": "tsc --noEmit",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:migrate": "npm run typeorm:migration:run",
    "db:migrate:revert": "npm run typeorm:migration:revert",
    "migrate": "node run-migration.js",
    "dev:setup": "npm run db:migrate && npm run dev",
    "prod:build": "npm run build",
    "env:validate": "node validate-env.js",
    "config:validate": "node -r ts-node/register src/scripts/validate-config.ts",
    "email:validate": "node validate-email-config.js",
    "typeorm:migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "typeorm:migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "typeorm:migration:show": "typeorm-ts-node-commonjs migration:show -d src/database/data-source.ts",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "typeorm:migration:create": "typeorm-ts-node-commonjs migration:create",
    "email:test": "node test-email.js"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@types/pdfkit": "^0.17.3",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "nodemailer": "^6.10.1",
    "oracledb": "^6.9.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.8.1",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.19",
    "@types/oracledb": "^6.9.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.2",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
